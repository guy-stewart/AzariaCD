
//-------Dice drop
//Clean up
warp(IDV_TRAYR);
setsleep(1);
grab(S16_DIEDROP);
drop(SMP_RECYCLE);
//Roll a new die
get(IDD_DICE);
drop(S16_DIEDROP);
setsleep(2);
grab(S16_DIEDROP);
drop(SMP_RECYCLE);

//Clean up
warp(IDV_TRAYL);
setsleep(1);
grab(S16_DICEROLL_R2);
drop(SMP_RECYCLE);
//Roll new dice
get(IDD_SHAKE);
drag(S16_DICEDROP_R2);
drop(S16_DICEDROP_R2);
setsleep(2);
grab(S16_DICEDROP_R2);
drop(SMP_RECYCLE);

///====================
//-------Memory Stone

warp(IDV_MEMSTONE);
setsleep(1);
grab(S27_MEMFUSE);
drop(SMP_RECYCLE);
grab(S27_MEMSPELL);
drop(SMP_RECYCLE);
//
get(IDD_MEMFUSE);
drop(S27_MEMFUSE);
setsleep(2);
get(IDD_MEMSPELL);
drop(S27_MEMSPELL);
setsleep(2);

grab(S27_MEMFULE);
drop(SMP_RECYCLE);
grab(S27_MEMSPELL);
drop(SMP_RECYCLE);

replay('hiders');










































