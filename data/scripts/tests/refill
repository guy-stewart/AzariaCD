

// fill up on nystrom
warp(S17_bMINE);
drop(SMP_RECYCLE);
grab(SMP_VIAL);
if (got() == IDD_SCOOPF) { drag(SMP_VIAL);}

write('got ', got());
while (got() == IDD_SCOOPE) {
    drop(SMP_RECYCLE);
    get(IDD_NYBREATH);
    drop(SID_SPELL);
    drop(SMP_RECYCLE);
    get(IDD_SCOOPE);
    drag(S17_bMINE);
    drag(SMP_VIAL);
    write('got ', got());
}
warp(IDV_QUARSA);